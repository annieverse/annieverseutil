{
  "name": "annieverseutil",
  "version": "0.1.2",
  "description": "A repo designed to house all util functionality for discord bot Annie",
  "license": "MIT",
  "author": "thefryignpan",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annieverse/annieverseutil.git"
  },
  "engines": {
    "node": ">=24.4.1",
    "npm": "^11.4.2"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/cjs/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "test": "eslint",
    "test:fix": "eslint --fix",
    "jest": "jest --experimental-vm-modules node_modules/.bin/jest ",
    "jest:coverage": "jest --experimental-vm-modules node_modules/.bin/jest --coverage",
    "patch:build": "npm run build && npm run patch",
    "minor:build": "npm run build && npm run minor",
    "major:build": "npm run build && npm run major",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "publish": "npm publish"
  },
  "contributors": [
    "Naph (https://github.com/klerikdust)",
    "Pan (https://github.com/Moore2021)"
  ],
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jest/globals": "^30.0.5",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/jest": "^30.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "typescript": "^5.9.2"
  }
}