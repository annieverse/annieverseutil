{
  "name": "@annieverse/annieverseutil",
  "version": "1.0.5",
  "description": "A repo designed to house all util functionality for discord bot Annie",
  "license": "MIT",
  "author": "thefryingpan",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annieverse/annieverseutil.git"
  },
  "engines": {
    "node": ">=24.4.1",
    "npm": "^11.4.2"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "scripts": {
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "patch:build": "npm run build && npm run patch",
    "minor:build": "npm run build && npm run minor",
    "major:build": "npm run build && npm run major",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project .configs/tsconfig.cjs.json",
    "build:esm": "tsc --project .configs/tsconfig.esm.json",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "contributors": [
    "Naph (https://github.com/klerikdust)",
    "Pan (https://github.com/Moore2021)"
  ],
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jest/globals": "^30.0.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/jest": "^30.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
